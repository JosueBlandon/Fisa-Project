<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FS_CreacionProducto</fullName>
    <active>true</active>
    <description>Regla que valida campos requeridos al momento de crear el producto</description>
    <errorConditionFormula>AND(
ISNEW(),
FS_Esta_activo__c = False
)</errorConditionFormula>
    <errorDisplayField>FS_Esta_activo__c</errorDisplayField>
    <errorMessage>Debe completar el campo ¿Está activo?</errorMessage>
</ValidationRule>
